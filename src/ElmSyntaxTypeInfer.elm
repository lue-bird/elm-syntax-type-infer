        [] ->
            if substitutions.variableToType |> FastDict.isEmpty then
                Ok patternTypedNode

            else
                case
                    substitutions.variableToType
                        |> substitutionsVariableToTypeApplyOverItself typeContext
                of
                    Err error ->
